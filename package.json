    "expo-web-browser": "~14.0.1",
    "jwt-decode": "^4.0.0",
    "lottie-react-native": "7.1.0",
    "lucide-react": "^0.468.0",
    "lucide-react-native": "^0.469.0",
    "moti": "^0.29.0",
    "react": "^18.3.1",
