{
  "name": "nomad-crew-frontend",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start -c",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "maestro test .maestro/flows/",
    "test:e2e:smoke": "maestro test .maestro/flows/smoke-test.yaml",
    "lint": "expo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "eas-build-pre-install": "npm ci --legacy-peer-deps || npm install --legacy-peer-deps",
    "prepare": "husky",
    "postinstall": "patch-package"
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@casl/react": "^5.0.0",
    "@expo/vector-icons": "^14.0.2",
    "@invertase/react-native-apple-authentication": "^2.4.0",
    "@lottiefiles/dotlottie-react": "^0.13.5",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-google-signin/google-signin": "^13.2.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@shopify/flash-list": "^2.2.0",
    "@supabase/supabase-js": "^2.86.2",
    "@tanstack/query-sync-storage-persister": "^5.90.14",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-persist-client": "^5.90.14",
    "assert": "^2.1.0",
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "base64-js": "^1.5.1",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "eventemitter3": "^5.0.1",
    "expo": "~52.0.46",
    "expo-apple-authentication": "^7.1.3",
    "expo-auth-session": "~6.0.1",
    "expo-blur": "~14.0.3",
    "expo-build-properties": "~0.13.3",
    "expo-constants": "~17.0.3",
    "expo-crypto": "~14.0.1",
    "expo-dev-client": "~5.0.14",
    "expo-dev-menu": "^6.0.21",
    "expo-device": "~7.0.3",
    "expo-font": "~13.0.2",
    "expo-google-places-autocomplete": "^1.2.0",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.3",
    "expo-location": "~18.0.10",
    "expo-notifications": "~0.29.14",
    "expo-router": "~4.0.21",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.1",
    "jwt-decode": "^4.0.0",
    "lottie-react-native": "7.1.0",
    "lucide-react": "^0.468.0",
    "lucide-react-native": "^0.469.0",
    "moti": "^0.29.0",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-bento": "^1.0.9",
    "react-dom": "^18.3.1",
    "react-native": "0.76.7",
    "react-native-autocomplete-dropdown": "^5.0.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-google-places-autocomplete": "^2.5.7",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-maps": "^1.18.0",
    "react-native-onboarding-swiper": "^1.3.0",
